








package org.plazy.evs {
	
	public class EvItem {
		
		public var hr:Function;
		public var priority:int;
		public var repeats:int;
		
		public function EvItem () { }
		
		public function toString ():String {
			return '{EvItem: hr=' + hr + ' priority=' + priority + ' repeats=' + repeats + '}';
		}
		
	}
	
}









